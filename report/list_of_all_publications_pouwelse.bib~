@article{Delaviz2010ImprovedPDS,
	author          = {Rahim Delaviz, Nazareno Andrade, D.H.J. Epema, and Johan Pouwelse},
	title           = {Improved Accuracy and Coverage in  the BarterCast Reputation Mechanism},
	journal         = {Proceedings of the sixteenth annual conference of the Advanced School for Computing and Imaging, Veldhoven, the Netherlands, November 1-3,},
	year            = {2010},
	pages           = {1-8},
	issn            = {978-90-79982-08-0},
	topic           = {Peer-to-Peer},
